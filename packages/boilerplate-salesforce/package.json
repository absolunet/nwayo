{
  "name": "@nwayo-project/MY_PROJECT",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "postinstall": "cd src && npm install && npm install",
    "build:all:production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=@nwayo/api/setup/webpack.config.js --env.nwayo.type=scripts --env.nwayo.type=styles --env.nwayo.type=assets --env.nwayo.bundle=site",
    "build:all": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=@nwayo/api/setup/webpack.config.js --env.nwayo.type=scripts --env.nwayo.type=styles --env.nwayo.type=assets --env.nwayo.bundle=site",
    "build:watch": "npm run build:all -- --watch",
    "test": "npm run test:standards && npm run test:unit",
    "test:standards": "npm run test:standards:scripts && npm run test:standards:styles",
    "test:standards:scripts": "eslint \"src/**/*.js\" --ignore-pattern \"src/node_modules/*\"",
    "test:standards:styles": "stylelint \"src/**/*.scss\" --ignore-pattern \"src/node_modules/*\"",
    "test:unit": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@absolunet/eslint-config-browser": "1.5.1",
    "@absolunet/eslint-config-node": "1.5.1",
    "@absolunet/fss": "1.6.2",
    "@absolunet/stylelint-config-nwayo": "1.3.1",
    "@babel/compat-data": "7.10.5",
    "@babel/plugin-transform-runtime": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "autoprefixer": "9.8.6",
    "babel-jest": "26.3.0",
    "coffee-loader": "1.0.0",
    "coffeescript": "2.5.1",
    "core-js": "3.6.5",
    "cross-env": "6.0.3",
    "crypto": "1.0.1",
    "css-loader": "3.6.0",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "jest": "26.4.2",
    "laravel-mix": "5.0.4",
    "laravel-mix-eslint": "0.1.3",
    "laravel-mix-imagemin": "1.0.3",
    "laravel-mix-stylelint": "0.2.0",
    "less": "3.12.2",
    "less-loader": "6.2.0",
