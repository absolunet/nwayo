# http://editorconfig.org

[*.{js,md,gitignore}]
indent_style = tab
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true


[{.editorconfig,.eslintrc.yaml,.travis.yml,package.json,sass.rb}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
